schema="EIMTRD";
query="SELECT TSP_ID, TS_NAME, PRO_NAME, PRO_VER, PLAT_NAME, OS_NAME, CYC_TAG, ASSGINEE, FEATURE, CATALOG
FROM EIMTRD.ETRD_TEST_PLAN TP INNER JOIN EIMTRD.ETRD_PRODUCTS P ON TP.PRO_ID = P.PRO_ID
                                  INNER JOIN EIMTRD.ETRD_TEST_CYCLES TC ON TP.CYC_ID = TC.CYC_ID";